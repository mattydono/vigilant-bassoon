    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "react-scroll-to-bottom": "^1.1.0",
    "react-scroll-to-component": "^1.0.2",
    "react-scrollable-feed": "^1.0.1",
    "redux": "^4.0.1",
    "uuid": "^3.3.2"
